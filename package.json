{
  "name": "taoland",
  "version": "1.0.0",
  "description": "a front-end blog",
  "main": "index.js",
  "scripts": {
    "dev": "cnpm i && npm-run-all --parallel dev:*",
    "dev:client": "cd frontend && node dev/server.js",
    "dev:server": "cd node && cross-env nodemon api/index.ts",
    "build": "cnpm i && npm-run-all --parallel build:*",
    "build:client": "cd frontend && del .next && next build",
    "build:server": "cd node && del api-dist && babel api --extensions '.ts' --out-dir api-dist",
    "start": "cnpm i && npm-run-all --parallel start:*",
    "start:client": "cd frontend && next start -p 7999",
    "start:server": "cd node && node api-dist/index.js",
    "docker-next-start": "next build && next start -p 7999",
    "docker-node-start": "babel api --extensions '.ts' --out-dir api-dist && node api-dist/index.js",
    "export": "cnpm i && npm-run-all --parallel export:*",
    "export:client": "del html && cd frontend && del .next && next build && next export -o ../html",
    "export:server": "cd node && del api-dist && babel api --extensions '.ts' --out-dir api-dist",
    "new": "cd frontend && plop",
    "lint": "./node_modules/.bin/eslint --cache --ext tsx,ts --fix frontend"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.11.0",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.38",
    "axios": "^0.19.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "localforage": "^1.7.3",
    "log4js": "^6.1.0",
    "mongoose": "^5.8.4",
    "next": "^9.1.6",
    "next-pwa": "^1.1.1",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^4.0.1",
    "nodemon": "^2.0.2",
    "notistack": "^0.9.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.0",
    "react-redux": "^7.1.3",
    "react-syntax-highlighter": "^12.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/koa": "^2.11.0",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-redux": "^7.1.5",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@zeit/next-less": "^1.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "compression-webpack-plugin": "^3.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "less": "^3.10.3",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.0",
    "plop": "^2.5.3",
    "redux-logger": "^3.0.6"
  }
}
